{
  "name": "html2halvalla",
  "description": "Converts HTML to halvalla markup",
  "author": "James Hinds <jahbini@icloud.com>",
  "contributors": "Brandon Bloom <brandon@brandonbloom.name>",
  "version": "1.5.2",
  "repository": {
    "type": "git",
    "url": "git://github.com/jahbini/html2halvalla.git"
  },
  "keywords": [
    "coffeekup",
    "teacup",
    "halvalla",
    "site-master"
  ],
  "bin": {
    "html2halvalla": "./bin/html2halvalla"
  },
  "scripts": {
    "decode": "rm -rf test/blov; bin/html2halvalla --b=header --m='<header' --b=footer --m='<footer' --b=blovi-ation --m='div class=\"large-8 columns\"' --s=test/blov  test/bs.html"
  },
  "main": "./lib/convert.coffee",
  "dependencies": {
    "coffee-script": ">= 1.1.1",
    "htmlparser": ">= 1.7.3",
    "underscore": "^1.8.3"
  },
  "engines": {
    "node": ">= 0.4.7"
  },
  "license": "BSD",
  "devDependencies": {
    "halvalla": "^1.8.3"
  }
}
