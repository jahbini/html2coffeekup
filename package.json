{
  "name": "html2coffeekup",
  "description": "Converts HTML to Coffeekup markup",
  "author": "Brandon Bloom <brandon@brandonbloom.name>",
  "version": "1.4.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/brandonbloom/html2coffeekup.git"
  },
  "keywords": [
    "coffeekup"
  ],
  "bin": {
    "html2coffeekup": "./bin/html2coffeekup"
  },
  "scripts": {
  "decode": "rm -rf test/blov; bin/html2coffeekup --b=header --m='<header' --b=footer --m='<footer' --b=bloviation --m='div class=\"large-8 columns\"' --s=test/blov  test/bs.html"
  },
  "main": "./lib/convert.coffee",
  "dependencies": {
    "coffee-script": ">= 1.1.1",
    "htmlparser": ">= 1.7.3",
    "underscore": "^1.8.3"
  },
  "engines": {
    "node": ">= 0.4.7"
  },
  "license": "BSD"
}
